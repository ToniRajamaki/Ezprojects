{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "src/App.js": true,
    ".gitignore": true,
    "package-lock.json": true,
    "package.json": true,
    "README.md": true,
    "src/Components/Restaurant_src": true,
    "src/Components/Tours_src": true
  },
  "hide-files.files": [
    "src/App.js",
    ".gitignore",
    "package-lock.json",
    "package.json",
    "README.md",
    "src/Components/Restaurant_src",
    "src/Components/Tours_src"
  ],
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#06ff27",
    "editor.selectionForeground": "#06ff27",
    "editor.wordHighlightStrongBorder": "#09ff0079",
    "editor.wordHighlightStrongBackground": "#09ff0000",
    "editor.selectionHighlightBackground": "#f0f5f138",
    "editor.wordHighlightBorder": "#09ff0079",
    "editor.wordHighlightBackground": "#15ff0000",
    "editor.findMatchHighlightBorder": "#00ff0075",
    "editorCursor.foreground": "#ffff00"
  }
}
